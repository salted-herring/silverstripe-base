        "heyday/silverstripe-optimisedimage": "~2.2.1",
        "hubertusanton/silverstripe-seo": "~2.0",
        "kinglozzer/silverstripegmapsobject": "~1.0.1",
        "normann/gridfieldpaginatorwithshowall": "dev-master",
