    "require": {
        "php": ">=5.3.2",
        "mobiledetect/mobiledetectlib": "~2.8.12",

        "silverstripe/cms": "~3.4.3",
        "silverstripe/framework": "~3.4.3",
        "silverstripe/googlesitemaps": "~1.2.2",
        "silverstripe/html5": "~1.0.2",
        "silverstripe-australia/gridfieldextensions": "~1.3.0",
